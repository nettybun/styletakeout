{
  "name": "styletakeout.macro",
  "version": "1.0.0",
  "description": "Take CSS out of JS at compile time",
  "main": "dist/styletakeout.macro.js",
  "types": "index.d.ts",
  "keywords": [
    "css",
    "styled-components",
    "babel-plugin-macros"
  ],
  "author": "Gen Hames",
  "license": "MIT",
  "scripts": {
    "build": "babel styletakeout.macro.ts --out-dir dist --extensions .ts"
  },
  "dependencies": {
    "babel-plugin-macros": "^2.8.0",
    "common-tags": "^1.8.0",
    "cssbeautify": "^0.3.1",
    "stylis": "^4.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/plugin-transform-typescript": "^7.10.4",
    "@types/babel-plugin-macros": "^2.8.2",
    "@types/common-tags": "^1.8.0",
    "@types/cssbeautify": "^0.3.1"
  }
}
